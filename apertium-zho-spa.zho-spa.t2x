<?xml version="1.0" encoding="UTF-8"?>
<!-- -*- nxml -*- -->
<interchunk>
  <section-def-cats>
    <def-cat n="SN-SUBJ">
      <cat-item lemma="subj" tags="SN.*"/>
    </def-cat>
    <def-cat n="SN">
      <cat-item tags="SN"/>
      <cat-item tags="SN.*"/>
    </def-cat>
    <def-cat n="SV">
      <cat-item tags="SV.*"/>
    </def-cat>
    <def-cat n="ADVL">
      <cat-item tags="ADV"/>
      <cat-item tags="ADV.*"/>
       <cat-item tags="SP"/>
      <cat-item tags="SP.*"/>
    </def-cat>
    <def-cat n="SP">
      <cat-item tags="SP"/>
      <cat-item tags="SP.*"/>
    </def-cat>

     <def-cat n="ADJ">
      <cat-item tags="SA"/>
      <cat-item tags="SA.*"/>
    </def-cat>

    <def-cat n="SENT">
      <cat-item tags="SENT"/>
    </def-cat>
  </section-def-cats>
  <section-def-attrs>
    <def-attr n="a_tns">
      <attr-item tags="pri"/>
      <attr-item tags="pii"/>
      <attr-item tags="inf"/>
      <attr-item tags="ifi"/>
      <attr-item tags="ZD"/>
    </def-attr>

    <def-attr n="a_pers">
      <attr-item tags="p1"/>
      <attr-item tags="p2"/>
      <attr-item tags="p3"/>
      <attr-item tags="PD"/>
    </def-attr>


    <def-attr n="a_gen">
      <attr-item tags="m"/>
      <attr-item tags="f"/>
      <attr-item tags="mf"/>
      <attr-item tags="GD"/>
    </def-attr>

    <def-attr n="a_nbr">
      <attr-item tags="sg"/>
      <attr-item tags="pl"/>
      <attr-item tags="sp"/>
      <attr-item tags="ND"/>
    </def-attr>

  </section-def-attrs>
  <section-def-vars>
    <def-var n="number"/>
    <def-var n="POST"/>
  </section-def-vars>
  <section-def-macros>
    <def-macro n="f_concord1" npar="2">
      <let><clip pos="2" part="a_pers"/><clip pos="1" part="a_pers"/></let>
      <let><clip pos="2" part="a_nbr"/><clip pos="1" part="a_nbr"/></let>
    </def-macro>
    <def-macro n="f_concord2" npar="2">
      <let><clip pos="2" part="a_gen"/><clip pos="1" part="a_gen"/></let>
      <let><clip pos="2" part="a_nbr"/><clip pos="1" part="a_nbr"/></let>
    </def-macro>
    <def-macro n="f_tense1" npar="1">
      <let>
        <clip pos="1" part="a_tns"/><lit-tag v="pri"/>
      </let>
    </def-macro>
    <def-macro n="f_inf1" npar="1">                                  <!--posa a inf els verbs i obvia pers i nbr. Aixo serveix per exemple amb vbmod + verbinf -->
        <let><clip pos="1" part="a_tns"/><lit-tag v="inf"/></let>
        <let><clip pos="1" part="a_pers"/><lit v=""/></let>
        <let><clip pos="1" part="a_nbr"/><lit v=""/></let>
    </def-macro>


  </section-def-macros>
  <section-rules>
    <rule comment="REGLA: SN">
      <pattern>
        <pattern-item n="SN"/>
      </pattern>
      <action>
        <out>
          <chunk>
            <clip pos="1" part="lem"/>
            <clip pos="1" part="tags"/>
            <clip pos="1" part="chcontent"/>
          </chunk>
        </out>
      </action>
    </rule>
    
    <rule comment="REGLA: SN SV">
      <pattern>
        <pattern-item n="SN"/>
        <pattern-item n="SV"/>
      </pattern>
      <action>
         <call-macro n="f_concord1">
          <with-param pos="1"/>
          <with-param pos="2"/>
        </call-macro>
        <call-macro n="f_tense1">
          <with-param pos="2"/>
        </call-macro>

        <out>
          <chunk>
            <clip pos="1" part="lem"/>
            <clip pos="1" part="tags"/>
            <clip pos="1" part="chcontent"/>
          </chunk>
           <b pos="1"/>
           <chunk>
            <clip pos="2" part="lem"/>
            <clip pos="2" part="tags"/>
            <clip pos="2" part="chcontent"/>
          </chunk>

        </out>
      </action>
    </rule>


    <rule comment="REGLA: SN-SUBJ ADVL SV ">
      <pattern>
        <pattern-item n="SN-SUBJ"/>
        <pattern-item n="ADVL"/>
        <pattern-item n="SV"/>
      </pattern>
      <action>
        <call-macro n="f_concord1">
          <with-param pos="1"/>
          <with-param pos="3"/>
        </call-macro>
        <call-macro n="f_tense1">
          <with-param pos="3"/>
        </call-macro>
        <out>
          <chunk>
            <clip pos="1" part="lem"/>
            <clip pos="1" part="tags"/>
            <clip pos="1" part="chcontent"/>
          </chunk>
          <b pos="1"/>
          <chunk>
            <clip pos="2" part="lem"/>
            <clip pos="2" part="tags"/>
            <clip pos="2" part="chcontent"/>
          </chunk>
          <b pos="2"/>
          <chunk>
            <clip pos="3" part="lem"/>
            <clip pos="3" part="tags"/>
            <clip pos="3" part="chcontent"/>
          </chunk>
        </out>
      </action>
    </rule>
    
    <rule comment="REGLA: SN ADVL SV ">
      <pattern>
        <pattern-item n="SN"/>
        <pattern-item n="ADVL"/>
        <pattern-item n="SV"/>
      </pattern>
      <action>
        <call-macro n="f_concord1">
          <with-param pos="1"/>
          <with-param pos="3"/>
        </call-macro>
        <call-macro n="f_tense1">
          <with-param pos="3"/>
        </call-macro>
        <out>
          <chunk>
            <clip pos="1" part="lem"/>
            <clip pos="1" part="tags"/>
            <clip pos="1" part="chcontent"/>
          </chunk>
          <b pos="1"/>
          <chunk>
            <clip pos="2" part="lem"/>
            <clip pos="2" part="tags"/>
            <clip pos="2" part="chcontent"/>
          </chunk>
          <b pos="2"/>
          <chunk>
            <clip pos="3" part="lem"/>
            <clip pos="3" part="tags"/>
            <clip pos="3" part="chcontent"/>
          </chunk>
        </out>
      </action>
    </rule>


    <rule comment="REGLA: SN-SUBJ ADVL ADVL SV ">
      <pattern>
        <pattern-item n="SN-SUBJ"/>
        <pattern-item n="ADVL"/>
        <pattern-item n="ADVL"/>
        <pattern-item n="SV"/>
      </pattern>
      <action>
        <call-macro n="f_concord1">
          <with-param pos="1"/>
          <with-param pos="4"/>
        </call-macro>
        <call-macro n="f_tense1">
          <with-param pos="4"/>
        </call-macro>
        <out>
          <chunk>
            <clip pos="1" part="lem"/>
            <clip pos="1" part="tags"/>
            <clip pos="1" part="chcontent"/>
          </chunk>
          <b pos="1"/>
          <chunk>
            <clip pos="2" part="lem"/>
            <clip pos="2" part="tags"/>
            <clip pos="2" part="chcontent"/>
          </chunk>
          <b pos="2"/>
          <chunk>
            <clip pos="3" part="lem"/>
            <clip pos="3" part="tags"/>
            <clip pos="3" part="chcontent"/>
          </chunk>
          <b pos="3"/>
          <chunk>
            <clip pos="4" part="lem"/>
            <clip pos="4" part="tags"/>
            <clip pos="4" part="chcontent"/>
          </chunk>
        </out>
      </action>
    </rule>
    
    
    <rule comment="REGLA: SN-SUBJ SV ">  <!--ellos NO pueden -->
      <pattern>
        <pattern-item n="SN-SUBJ"/>
        <pattern-item n="SV"/>
      </pattern>
      <action>
        <call-macro n="f_concord1">
          <with-param pos="1"/>
          <with-param pos="2"/>
        </call-macro>
        <call-macro n="f_tense1">
          <with-param pos="2"/>
        </call-macro>
        <out>
          <chunk>
            <clip pos="1" part="lem"/>
            <clip pos="1" part="tags"/>
            <clip pos="1" part="chcontent"/>
          </chunk>
          <b pos="1"/>
   
          <chunk>
            <clip pos="2" part="lem"/>
            <clip pos="2" part="tags"/>
            <clip pos="2" part="chcontent"/>
          </chunk>
        </out>
      </action>
    </rule>

    
    <rule comment="REGLA: SN-SUBJ SV SP SV ">  <!--Ellos no pueden en exterior jugar--><!-- En aquest cas ADVL inclou SP i adv, com que en t1x hem agrupat prep+nom com a SP per aixo funciona-->
      <pattern>
        <pattern-item n="SN-SUBJ"/>
        <pattern-item n="SV"/>
        <pattern-item n="SP"/>
        <pattern-item n="SV"/>
      </pattern>
      <action>
        <call-macro n="f_concord1">
          <with-param pos="1"/>
          <with-param pos="2"/>
        </call-macro>
        <call-macro n="f_tense1">
          <with-param pos="2"/>
        </call-macro>
        <call-macro n="f_inf1">
          <with-param pos="4"/>
        </call-macro>
        <out>
          <chunk>
            <clip pos="1" part="lem"/>
            <clip pos="1" part="tags"/>
            <clip pos="1" part="chcontent"/>
          </chunk>
          <b pos="1"/>
          <chunk>
            <clip pos="2" part="lem"/>
            <clip pos="2" part="tags"/>
            <clip pos="2" part="chcontent"/>
          </chunk>
          <b pos="2"/>
          <chunk>
            <clip pos="3" part="lem"/>
            <clip pos="3" part="tags"/>
            <clip pos="3" part="chcontent"/>
          </chunk>
          <b pos="3"/>
          <chunk>
            <clip pos="4" part="lem"/>
            <clip pos="4" part="tags"/>
            <clip pos="4" part="chcontent"/>
          </chunk>
        </out>
      </action>
    </rule>
    
    <rule comment="REGLA: SN-SUBJ ADVL SV SV ">  <!--Jaime y maria muy gusta jugar-->
      <pattern>
        <pattern-item n="SN-SUBJ"/>
        <pattern-item n="ADVL"/>
