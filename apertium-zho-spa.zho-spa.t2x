          <with-param pos="1"/>
          <with-param pos="2"/>
        </call-macro>
        <call-macro n="f_tense1">
          <with-param pos="2"/>
        </call-macro>

        <out>
          <chunk>
            <clip pos="1" part="lem"/>
            <clip pos="1" part="tags"/>
            <clip pos="1" part="chcontent"/>
          </chunk>
           <b pos="1"/>
           <chunk>
            <clip pos="2" part="lem"/>
            <clip pos="2" part="tags"/>
            <clip pos="2" part="chcontent"/>
          </chunk>

        </out>
      </action>
    </rule>


