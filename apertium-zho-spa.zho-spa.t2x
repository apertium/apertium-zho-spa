    <rule comment="REGLA: SN-SUBJ ADVL SP SP SP ADJ SV ">  <!--ellos, siempre, en grande casa, en frente de, de jardin, juntos, juegan-->
      <pattern>
        <pattern-item n="SN-SUBJ"/>
        <pattern-item n="ADVL"/>
        <pattern-item n="ADVL"/>
        <pattern-item n="ADVL"/>
        <pattern-item n="ADVL"/>
        <pattern-item n="ADJ"/>
        <pattern-item n="SV"/>
      </pattern>
      <action>
        <call-macro n="f_concord1"> <!--conconrdança pers i nbr amb subj-verb-->
          <with-param pos="1"/>
          <with-param pos="7"/>
        </call-macro>
        <call-macro n="f_concord2"> <!--conconrdança gen i nbr amb subj-adj-->
          <with-param pos="1"/>
          <with-param pos="6"/>
        </call-macro>
        <call-macro n="f_tense1">
          <with-param pos="7"/>
        </call-macro>
        <out>
          <chunk>
            <clip pos="1" part="lem"/>
            <clip pos="1" part="tags"/>
            <clip pos="1" part="chcontent"/>
          </chunk>
          <b pos="1"/>
          <chunk>
            <clip pos="2" part="lem"/>
            <clip pos="2" part="tags"/>
            <clip pos="2" part="chcontent"/>
          </chunk>
          <b pos="2"/>
          <chunk>
            <clip pos="3" part="lem"/>
            <clip pos="3" part="tags"/>
            <clip pos="3" part="chcontent"/>
          </chunk>
          <b pos="3"/>
          <chunk>
            <clip pos="4" part="lem"/>
            <clip pos="4" part="tags"/>
            <clip pos="4" part="chcontent"/>
          </chunk>
          <b pos="4"/>
          <chunk>
            <clip pos="5" part="lem"/>
            <clip pos="5" part="tags"/>
            <clip pos="5" part="chcontent"/>
          </chunk>
         <b pos="5"/>
          <chunk>
            <clip pos="6" part="lem"/>
            <clip pos="6" part="tags"/>
            <clip pos="6" part="chcontent"/>
          </chunk>
          <b pos="6"/>
          <chunk>
            <clip pos="7" part="lem"/>
            <clip pos="7" part="tags"/>
            <clip pos="7" part="chcontent"/>
          </chunk>
        </out>
      </action>
    </rule>
    
     <rule comment="REGLA: SN ADVL SV SP SV "> <!--pequeño perro muy gusta con Jaime y Maria jugar-->
      <pattern>
        <pattern-item n="SN"/>
        <pattern-item n="ADVL"/>
        <pattern-item n="SV"/>
        <pattern-item n="SP"/>
        <pattern-item n="SV"/>
      </pattern>
      <action>
        <call-macro n="f_concord1">
          <with-param pos="1"/>
