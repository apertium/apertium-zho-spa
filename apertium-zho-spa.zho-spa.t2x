<?xml version="1.0" encoding="UTF-8"?>
<!-- -*- nxml -*- -->
<interchunk>
  <section-def-cats>
    <def-cat n="SN-SUBJ">
      <cat-item lemma="subj" tags="SN.*"/>
      <cat-item lemma="subj" tags="SN"/>

    </def-cat>
    <def-cat n="SN">
      <cat-item tags="SN"/>
      <cat-item tags="SN.*"/>
    </def-cat>
    <def-cat n="SV">
      <cat-item tags="SV.*"/>
    </def-cat>
    <def-cat n="ADVL">
      <cat-item tags="ADV"/>
      <cat-item tags="ADV.*"/>
       <cat-item tags="SP"/>
      <cat-item tags="SP.*"/>
    </def-cat>
    <def-cat n="SP">
      <cat-item tags="SP"/>
      <cat-item tags="SP.*"/>
    </def-cat>

     <def-cat n="ADJ">
      <cat-item tags="SA"/>
      <cat-item tags="SA.*"/>
    </def-cat>
    <def-cat n="ADJmf">
      <cat-item tags="SAmf"/>
      <cat-item tags="SAmf.*"/>
    </def-cat>
     <def-cat n="cnjcoo">
      <cat-item tags="cnjcoo"/>
    </def-cat>
    

    <def-cat n="SENT">
      <cat-item tags="SENT"/>
    </def-cat>
  </section-def-cats>
  <section-def-attrs>
    <def-attr n="a_tns">
      <attr-item tags="pri"/>
      <attr-item tags="pii"/>
      <attr-item tags="inf"/>
      <attr-item tags="ifi"/>
      <attr-item tags="ZD"/>
    </def-attr>

    <def-attr n="a_pers">
      <attr-item tags="p1"/>
      <attr-item tags="p2"/>
      <attr-item tags="p3"/>
      <attr-item tags="PD"/>
    </def-attr>


    <def-attr n="a_gen">
      <attr-item tags="m"/>
      <attr-item tags="f"/>
      <attr-item tags="mf"/>
      <attr-item tags="GD"/>
    </def-attr>

    <def-attr n="a_nbr">
      <attr-item tags="sg"/>
      <attr-item tags="pl"/>
      <attr-item tags="sp"/>
      <attr-item tags="ND"/>
    </def-attr>

  </section-def-attrs>
  <section-def-vars>
    <def-var n="number"/>
    <def-var n="POST"/>
  </section-def-vars>
  <section-def-macros>
    <def-macro n="f_concord1" npar="2">
      <let><clip pos="2" part="a_pers"/><clip pos="1" part="a_pers"/></let>
      <let><clip pos="2" part="a_nbr"/><clip pos="1" part="a_nbr"/></let>
    </def-macro>
    <def-macro n="f_concord2" npar="2">
      <let><clip pos="2" part="a_gen"/><clip pos="1" part="a_gen"/></let>
      <let><clip pos="2" part="a_nbr"/><clip pos="1" part="a_nbr"/></let>
    </def-macro>
    <def-macro n="f_concord3" npar="2">
      <let><clip pos="2" part="a_nbr"/><clip pos="1" part="a_nbr"/></let>
    </def-macro>
    
    <def-macro n="f_tense1" npar="1">
      <let>
        <clip pos="1" part="a_tns"/><lit-tag v="pri"/>
      </let>
    </def-macro>
    <def-macro n="f_inf1" npar="1">                                  <!--posa a inf els verbs i obvia pers i nbr. Aixo serveix per exemple amb vbmod + verbinf -->
        <let><clip pos="1" part="a_tns"/><lit-tag v="inf"/></let>
        <let><clip pos="1" part="a_pers"/><lit v=""/></let>
        <let><clip pos="1" part="a_nbr"/><lit v=""/></let>
    </def-macro>
    <def-macro n="f_pri1" npar="1">                                  <!--pone manos en frente de (sense el ella), per exemple-->
        <let><clip pos="1" part="a_tns"/><lit-tag v="pri"/></let>
        <let><clip pos="1" part="a_pers"/><lit-tag v="p3"/></let>
        <let><clip pos="1" part="a_nbr"/><lit-tag v="sg"/></let>
    </def-macro>
    <def-macro n="f_advadj" npar="1">                                  <!--pone manos en frente de (sense el ella), per exemple-->
        <let><clip pos="1" part="a_gen"/><lit-tag v="m"/></let>
        <let><clip pos="1" part="a_nbr"/><lit-tag v="sg"/></let>
    </def-macro>



  </section-def-macros>
  <section-rules>
    <rule comment="REGLA: SN">
